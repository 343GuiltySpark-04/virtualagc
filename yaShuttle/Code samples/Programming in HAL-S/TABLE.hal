# This sample appears on PDF p. 52 of "Programming in HAL/S".
# It illustrates macros.

  TABLE:
  PROGRAM;
     REPLACE LOG2(X) BY "LOG(X)/LOG(2)";
     REPLACE ENTRY(N) BY "WRITE(6) N, 2**(N-1), N/LOG2(10)";
     ENTRY(8);
     ENTRY(12);
     ENTRY(16);
     ENTRY(18);
     ENTRY(24);
     ENTRY(32);
     ENTRY(36);
  CLOSE TABLE;