  DATATYPES:
  PROGRAM;
     DECLARE S SCALAR;
     DECLARE I INTEGER;
     DECLARE V VECTOR;
     DECLARE M MATRIX;
E	 -   -
M    S = V . V;
E        -   -
M    V = V * V;
E    -   - *
M    V = V M;
E    *   - -
M    M = V V;
E    *   * *
M    M = M M;
E    -   -
M    V = V S;
  CLOSE DATATYPES;
  