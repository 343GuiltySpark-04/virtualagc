C/ From PDF p. 107 in "Programming in HAL/S".

  EXAMPLE_3:
  PROGRAM;
     DECLARE ARRAY(3, 3),
     		M1, M2, M3;
     DECLARE INTEGER,
     		ROW, COL;
     DO FOR ROW = 1 TO 3;
        DO FOR COL = 1 TO 3;
M	   M1        = M2      M3      + M2      M3      + M2      M3      ;
S	     ROW,COL     ROW,1   1,COL     ROW,2   2,COL     ROW,3   3,COL

	END;
     END;
  CLOSE EXAMPLE_3;
  